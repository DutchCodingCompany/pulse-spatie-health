{
    "name": "dutchcodingcompany/pulse-spatie-health",
    "description": " A Laravel Pulse card to show the results of the Spatie Health package. ",
    "keywords": [
        "DutchCodingCompany",
        "laravel",
        "pulse-spatie-health"
    ],
    "homepage": "https://github.com/dutchcodingcompany/pulse-spatie-health",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Raaijmakers",
            "email": "bram@dutchcodingcompany.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "laravel/pulse": "^1.0@beta",
        "spatie/laravel-health": "^1.27.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8|^8.1",
        "larastan/larastan": "^2.0.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "DutchCodingCompany\\PulseSpatieHealth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DutchCodingCompany\\PulseSpatieHealth\\PulseSpatieHealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
