{
    "name": "bramr94/pulse-spatie-health",
    "description": " A Laravel Pulse card to show the results of the Spatie Health package. ",
    "keywords": [
        "bramr94",
        "laravel",
        "pulse-spatie-health"
    ],
    "homepage": "https://github.com/bramr94/pulse-spatie-health",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Raaijmakers",
            "email": "bram@dutchcodingcompany.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "laravel/pulse": "^1.0@beta",
        "spatie/laravel-health": "^1.23",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "larastan/larastan": "^2.0.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Bramr94\\PulseSpatieHealth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bramr94\\PulseSpatieHealth\\PulseSpatieHealthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
